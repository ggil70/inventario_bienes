<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>
      

<record model="ir.ui.view" id="view_inventario_resultado_bien_form">
        <field name="name">inventario_resultado_bien_form</field>
        <field name="model">inventario_resultado_bien</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Resultado Inventario Del Bien">
                <sheet>
                 <notebook>
                     <page string="Resultados">   
                         <sheet>
                             <group col="2" colspan="4">
                                 <field name="nombre"/>
                             </group> 
                         </sheet>
                     </page>         
                    </notebook>
               </sheet>
            </form>
        </field>
</record>
    
    
<record model="ir.ui.view" id="view_inventario_resultado_bien_tree">

      <field name="name">inventario_resultado_bien_tree</field>
      <field name="model">inventario_resultado_bien</field>
      <field name="type">tree</field>
      <field name="arch" type="xml">
         <tree string="Resultado Inventario Del Bien">
             <field name="nombre"/>
         </tree>
     </field>

</record>

<record model="ir.actions.act_window" id="action_inventario_resultado_bien">
        <field name="name">Resultado Inventario Bien</field>
        <field name="res_model">inventario_resultado_bien</field>
        
        <field name="view_mode">tree,form</field>
</record>


